@model DataCollectionViewModel
@{

}
<div class="duyurular">
    <a asp-controller="Home" asp-action="Duyurular"><h5>DUYURULAR</h5></a>
    <ul>
        @foreach (var duyuru in Model.Duyurular)
        {
            <li>
                <a asp-controller="Home" asp-action="Duyuru" asp-route-id="@duyuru.Id">
                    @duyuru.DuyuruBaslik<span> : @duyuru.DuyuruAciklama</span>
                </a>
            </li>
        }
    </ul>
</div>

<div class="duyurular">
    <a asp-controller="Home" asp-action="Etkinlikler"><h5>ETKİNLİKLER</h5></a>
    <ul>
        @foreach (var etkinlik in Model.Etkinlikler)
        {
            <li>
                <a asp-controller="Home" asp-action="Etkinlik" asp-route-id="@etkinlik.Id">
                    @etkinlik.EtkinlikBaslik<span> : @etkinlik.EtkinlikAciklama</span>
                </a>
            </li>
        }
    </ul>
</div>